{
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "installCommand": "npm install --legacy-peer-deps",
  "build": {
    "env": {
      "CI": "false",
      "SKIP_PREFLIGHT_CHECK": "true"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/**.js": {
      "runtime": "nodejs18.x"
    }
  }
}